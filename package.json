{
  "name": "instill-sdk",
  "version": "0.0.4",
  "description": "Instill AI's Typescript SDK",
  "repository": "https://github.com/instill-ai/typescript-sdk.git",
  "bugs": "https://github.com/instill-ai/community/issues",
  "keywords": [
    "instill-ai",
    "SDK",
    "Typescript"
  ],
  "scripts": {
    "test": "vitest --run --passWithNoTests",
    "build": "pnpm rimraf dist && tsup src && pnpm ts-types && ./generate_proto.sh",
    "ts-types": "tsc --emitDeclarationOnly",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx --cache",
    "publish-rc": "pnpm build && pnpm publish --no-git-checks"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "license": "Apache-2.0",
  "private": false,
  "devDependencies": {
    "@instill-ai/eslint-config-cortex": "0.5.0",
    "@testing-library/jest-dom": "^6.4.5",
    "@vitejs/plugin-react": "^4.2.1",
    "esbuild": "^0.14.34",
    "eslint": "^8.35.0",
    "jsdom": "^24.0.0",
    "rimraf": "^3.0.2",
    "ts-proto": "^1.157.1",
    "tsup": "^6.7.0",
    "tsx": "^3.12.3",
    "typescript": "^5.1.0",
    "vitest": "^1.6.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@types/json-schema": "^7.0.11",
    "axios": "^1.6.8",
    "json-schema": "^0.4.0",
    "openapi-types": "^12.1.3"
  }
}
